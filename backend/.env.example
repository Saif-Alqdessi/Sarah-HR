# FastAPI / Backend Environment Variables
# Copy to .env and fill in your values

# Application
DEBUG=false

# Supabase (required for database)
# Use SERVICE_ROLE_KEY for backend - bypasses RLS. Get from Dashboard > Settings > API.
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your-anon-key
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key

# Vapi AI (voice interviews)
VAPI_API_KEY=your-vapi-api-key
VAPI_ASSISTANT_ID=your-assistant-id

# Gemini API (scoring & analysis)
GEMINI_API_KEY=your-gemini-api-key

# Deepgram (optional - backup STT)
DEEPGRAM_API_KEY=your-deepgram-api-key

# Groq API (for Whisper STT)
GROQ_API_KEY=your-groq-api-key
